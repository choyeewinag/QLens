{
  "name": "QLens",
  "version": "1.0.0",
  "description": "",
  "jest": {
    "globalSetup": "./jest-setup.js",
    "globalTeardown": "./jest-teardown.js",
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared",
      "src"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules",
      "\\.(css|less)$": "<rootDir>/test/jest/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "^react(.*)$": "<rootDir>/vendor/react-master$1",
      "^config$": "<rootDir>/configs/app-config.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-cookies)/)"
    ]
  },
  "main": "electron.js",
  "homepage": "./",
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack serve --hot --host localhost --config=./webpack.dev.config.js --mode development",
    "build": "webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds",
    "test": "jest --verbose",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenlabrie/QLens.git"
  },
  "keywords": [],
  "author": "Steven LaBrie, Jake Diorio, Judy Tan, Cho Yee Win Aung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevenlabrie/QLens/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babili-webpack-plugin": "^0.1.2",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "electron": "^11.3.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "mini-css-extract-plugin": "^1.3.5",
    "nodemon": "^2.0.7",
    "react-test-renderer": "^17.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "spectron": "^13.0.0",
    "style-loader": "^2.0.0",
    "use-resize-observer": "^7.0.0",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@testing-library/user-event": "^12.7.2",
    "ace-builds": "^1.4.12",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "codemirror": "^5.59.2",
    "cors": "^2.8.5",
    "d3": "^6.5.0",
    "dotenv": "^8.2.0",
    "electron-builder": "^22.9.1",
    "electron-fetch": "^1.7.3",
    "electron-is-dev": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "extract-mongo-schema": "^0.2.10",
    "fetch-mock": "^9.11.0",
    "graphiql": "^1.3.2",
    "graphql": "^15.5.0",
    "graphql-log": "^0.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-transform-css": "^2.1.0",
    "lodash": "^4.17.20",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^8.3.0",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.14",
    "msw": "^0.26.2",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-ace": "^9.3.0",
    "react-codemirror2": "^7.2.1",
    "react-d3-tree": "^2.0.1",
    "react-dom": "^17.0.1",
